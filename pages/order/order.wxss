/* pages/order/order.wxss */
.list{
  background:rgba(249,249,249,1);
  width: 94%;
  margin: 0 auto;
  overflow: hidden
}
.li{
  width: 100%;
  height: auto;
  margin-top: 20rpx;
  background: #fff;
  border-radius: 10rpx;
  overflow: hidden;
}
.product{
  width: 100%;
  height: 120rpx;
}
.product>image{
  float: left;
  display: inline-block;
  width: 120rpx;
  height: 120rpx;
  border-radius:10rpx;
}
.product>.dis{
  width: 500rpx;
  height: 100%;
  float: left;
  margin-left: 30rpx;
}
.product>.dis>.state{
  font-size:32rpx;
font-family:PingFang SC;
font-weight:bold;
color:rgba(249,50,81,1);
display: block
}
.product>.dis>.hao{
  font-size:26rpx;
font-family:PingFang SC;
font-weight:500;
color:rgba(26,26,26,1);
margin-top: 15rpx;
display: block

}
.product>.dis>.time{
  font-size:26rpx;
font-family:PingFang SC;
font-weight:500;
color:rgba(26,26,26,1);
margin-top: 8rpx;
display: block

}
.in>text{
  display: block;
  margin-top: 15rpx;
  font-size:26rpx;
font-family:PingFang SC;
font-weight:500;
color:rgba(102,102,102,1);

padding-bottom: 20rpx;
}